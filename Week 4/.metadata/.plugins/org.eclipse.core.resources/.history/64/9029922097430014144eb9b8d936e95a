package com.laurenelder.photolocationapp;

public class Data {
	
	public String title;
	public String info;
	public String latitude;
	public String longitude;
	public String image;
	
	public Data(String thisTitle, String thisInfo, String thisLatitude, 
			String thisLongitude, String thisImage) {
		title = thisTitle;
		info = thisInfo;
		latitude = thisLatitude;
		longitude = thisLongitude;
		image = thisImage;
	}
	
	public String toString() {
		return title;
	}
}
